*foreground: #fff6ff
*foreground_bold: #ffecff
*cursor: #f3ceff
*background:       #05080a
!!*background: rgba(0, 0, 0, .7)

!! black
*.color0:       #0A0D0F
*.color8:       #0f1214

!! red
*.color1:       #ff9fbc
*.color9:       #ffa4c1

!! green
*.color2:       #baf7b5
*.color10:      #bffcba

!! yellow
*.color3:       #ffc79b
*.color11:      #ffcca0

!! blue
*.color4:       #b2fff3
*.color12:      #b7fff8

!! purple
*.color5:       #dfbaff
*.color13:      #e4bfff

!! pink
*.color6:       #ffc2df
*.color14:      #ffc7e4

!! white
*color7:        #fff6ff
*color15:       #fffbff

URxvt.intensityStyles:	false
URxvt.background:       [90]#000000
URxvt.depth:		32
!!URxvt.font: 		xft:monospace:size=12
URxvt.font:         xft:RobotoMono Nerd Font:size=12
!!URxvt.font: 	  xft:Sarasa Mono Sc Nerd:size=12
!!URxvt.font:       xft:IBM Plex Mono:size=12
URxvt.scrollBar:	false
URxvt.cursorColor:	white
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 92x24
!URxvt.internalBorder: 24
URxvt.internalBorder: 4
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.iso14755: false

!! URxvt Functions
URxvt.perl-ext-common: default,font-size,selection-to-clipboard,pasta
URxvt.keysym.Shift-Control-C:    perl:keyboard-select:activate
!!URxvt.keysym.Shift-Control-V:    perl:pasta:paste
URxvt.keysym.Shift-Control-V:    eval:paste_clipboard
! Comment this if you don't want copy when text is selected
URxvt.clipboard.autocopy: true
URxvt.keysym.C-j:       perl:font-size:increase
URxvt.keysym.C-k:       perl:font-size:decrease
